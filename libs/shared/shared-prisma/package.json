{
  "name": "@my-relax-app/shared-prisma",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {},
  "nx": {
    "name": "shared-prisma",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/shared/shared-prisma",
          "tsConfig": "libs/shared/shared-prisma/tsconfig.lib.json",
          "packageJson": "libs/shared/shared-prisma/package.json",
          "main": "libs/shared/shared-prisma/src/index.ts",
          "assets": [
            "libs/shared/shared-prisma/*.md"
          ]
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@nestjs/common": "^10.0.0"
  }
}
